{
  "name": "micvital/slider-module",
  "type": "asgard-module",
  "description": "Slider module for AsgardCMS",
  "keywords": [
    "asgardcms",
    "slider"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "David Cap",
      "email": "david@bocian.cz",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@bocian.cz",
    "issues": "https://github.com/MicVital/slider-module/issues",
    "source": "https://github.com/MicVital/slider-module"
  },
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.0",
    "micvital/core-module": "~1.0",
    "idavoll/media-module": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*",
    "doctrine/dbal": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.7"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Slider\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "autoload": {
    "psr-4": {
      "Modules\\Slider\\": ""
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan module:migrate Slider",
      "php artisan module:publish Slider"
    ],
    "post-update-cmd": [
      "php artisan module:migrate Slider",
      "php artisan module:publish Slider"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
